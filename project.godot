; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GemcatcherCS"
run/main_scene="res://Scenes/game.tscn"
config/features=PackedStringArray("4.3", "C#", "Mobile")
config/icon="res://icon.svg"

[dotnet]

project/assembly_name="GemcatcherCS"

[editor]

naming/scene_name_casing=1
naming/script_name_casing=1

[global_group]

gems="gems that give +1 score to the player"
speed_changers="gems that change speed"
length_changers="gems that change length"

[rendering]

renderer/rendering_method="mobile"
